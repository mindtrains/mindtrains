<mindtrains>  <set>    <piece id="curve1" class="uk.co.mindtrains.Piece">      <image file="curve45-2.png" hflip="false" vflip="false" />      <connector x="8" y="46" orientation="W" />      <connector x="24" y="6" orientation="NW" />    </piece>    <piece id="curve2" class="uk.co.mindtrains.Piece">      <image file="curve45.png" hflip="false" vflip="false" />      <connector x="1" y="19" orientation="SE" />      <connector x="46" y="8" orientation="N" />    </piece>    <piece id="curve3" class="uk.co.mindtrains.Piece">      <image file="curve45.png" hflip="true" vflip="false" />      <connector x="0" y="0" orientation="S" />      <connector x="40" y="24" orientation="NE" />    </piece>    <piece id="curve4" class="uk.co.mindtrains.Piece">      <image file="curve45-2.png" hflip="true" vflip="false" />      <connector x="25" y="46" orientation="W" />      <connector x="6" y="1" orientation="SW" />    </piece>    <piece id="curve5" class="uk.co.mindtrains.Piece">      <image file="curve45-2.png" hflip="false" vflip="true" />      <connector x="0" y="0" orientation="E" />      <connector x="19" y="45" orientation="NE" />    </piece>    <piece id="curve6" class="uk.co.mindtrains.Piece">      <image file="curve45.png" hflip="false" vflip="true" />      <connector x="6" y="1" orientation="SW" />      <connector x="46" y="25" orientation="N" />    </piece>    <piece id="curve7" class="uk.co.mindtrains.Piece">      <image file="curve45.png" hflip="true" vflip="true" />      <connector x="0" y="17" orientation="S" />      <connector x="45" y="6" orientation="NW" />    </piece>    <piece id="curve8" class="uk.co.mindtrains.Piece">      <image file="curve45-2.png" hflip="true" vflip="true" />      <connector x="17" y="0" orientation="E" />      <connector x="1" y="40" orientation="SE" />    </piece>    <piece id="straight1" class="uk.co.mindtrains.Piece">      <image file="shorthorizstraight.png" hflip="false" vflip="false" />      <connector x="0" y="0" orientation="S" />      <connector x="35" y="8" orientation="N" />    </piece>    <piece id="straight2" class="uk.co.mindtrains.Piece">      <image file="straighthorizlong.png" hflip="false" vflip="false" />      <connector x="0" y="0" orientation="S" />      <connector x="50" y="8" orientation="N" />    </piece>    <piece id="straight3" class="uk.co.mindtrains.Piece">      <image file="shortvertstraight.png" hflip="false" vflip="false" />      <connector x="0" y="0" orientation="E" />      <connector x="8" y="35" orientation="W" />    </piece>    <piece id="straight4" class="uk.co.mindtrains.Piece">      <image file="straightvertlong.png" hflip="false" vflip="false" />      <connector x="0" y="0" orientation="E" />      <connector x="8" y="50" orientation="W" />    </piece>    <piece id="diagonal1" class="uk.co.mindtrains.Piece">      <image file="diagonalshort.png" hflip="false" vflip="false" />      <connector x="25" y="5" orientation="NW" />      <connector x="0" y="20" orientation="SE" />    </piece>    <piece id="diagonal2" class="uk.co.mindtrains.Piece">      <image file="diagonallong.png" hflip="false" vflip="false" />      <connector x="40" y="5" orientation="NW" />      <connector x="0" y="35" orientation="SE" />    </piece>    <piece id="diagonal3" class="uk.co.mindtrains.Piece">      <image file="diagonalshort.png" hflip="true" vflip="false" />      <connector x="5" y="0" orientation="SW" />      <connector x="20" y="25" orientation="NE" />    </piece>    <piece id="diagonal4" class="uk.co.mindtrains.Piece">      <image file="diagonallong.png" hflip="true" vflip="false" />      <connector x="5" y="0" orientation="SW" />      <connector x="35" y="40" orientation="NE" />    </piece>    <piece id="if1" class="uk.co.mindtrains.If">      <image file="if.png" hflip="false" vflip="false" />      <connector x="8" y="46" orientation="W" />      <connector x="24" y="6" orientation="NW" />      <connector x="0" y="11" orientation="E" />    </piece>    <piece id="if2" class="uk.co.mindtrains.If">      <image file="if.png" hflip="false" vflip="true" />      <connector x="0" y="0" orientation="E" />      <connector x="19" y="45" orientation="NE" />      <connector x="8" y="35" orientation="W" />    </piece>    <piece id="crane" class="uk.co.mindtrains.Increment">      <image file="crane.png" hflip="false" vflip="false" />      <connector x="0" y="0" orientation="E" />      <connector x="8" y="35" orientation="W" />    </piece>    <piece id="random" class="uk.co.mindtrains.Random">      <image file="random.png" hflip="false" vflip="false" />      <connector x="0" y="0" orientation="E" />      <connector x="8" y="35" orientation="W" />    </piece>    <piece id="input" class="uk.co.mindtrains.Piece">      <image file="input.png" hflip="false" vflip="false" />      <connector x="0" y="0" orientation="E" />      <connector x="8" y="35" orientation="W" />    </piece>    <piece id="output" class="uk.co.mindtrains.Piece">      <image file="output.png" hflip="false" vflip="false" />      <connector x="0" y="0" orientation="E" />      <connector x="8" y="35" orientation="W" />    </piece>    <piece id="main" class="uk.co.mindtrains.Main">      <image file="tunnel.png" hflip="false" vflip="false" />      <connector x="13" y="16" orientation="W" />    </piece>  </set>  <palette>    <tab name="track">      <piece ref="curve1" />      <piece ref="curve2" />      <piece ref="curve3" />      <piece ref="curve4" />      <piece ref="curve5" />      <piece ref="curve6" />      <piece ref="curve7" />      <piece ref="curve8" />      <piece ref="straight1" />      <piece ref="straight2" />      <piece ref="straight3" />      <piece ref="straight4" />      <piece ref="diagonal1" />      <piece ref="diagonal2" />      <piece ref="diagonal3" />      <piece ref="diagonal4" />    </tab>        <tab name="switches">      <piece ref="if1" />      <piece ref="if2" />      <piece ref="crane" />      <piece ref="random" />      <piece ref="input" />      <piece ref="output" />    </tab>  </palette>    <land>    <piece ref="main" x="50" y="50" fixed="true" />  </land></mindtrains>