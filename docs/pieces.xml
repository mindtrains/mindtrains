<mindtrains>  <set>    <piece name="curve1" class="piece">      <image file="curve45-2.png" hflip="false" vflip="false" />      <connector x="8" y="46" orientation="W" />      <connector x="24" y="6" orientation="NW" />    </piece>    <piece name="curve2" class="piece">      <image file="curve45.png" hflip="false" vflip="false" />      <connector x="1" y="19" orientation="SE" />      <connector x="46" y="8" orientation="N" />    </piece>    <piece name="curve3" class="piece">      <image file="curve45.png" hflip="true" vflip="false" />      <connector x="0" y="0" orientation="S" />      <connector x="40" y="24" orientation="NE" />    </piece>    <piece name="curve4" class="piece">      <image file="curve45-2.png" hflip="true" vflip="false" />      <connector x="25" y="46" orientation="W" />      <connector x="6" y="1" orientation="SW" />    </piece>    <piece name="curve5" class="piece">      <image file="curve45-2.png" hflip="false" vflip="true" />      <connector x="0" y="0" orientation="E" />      <connector x="19" y="45" orientation="NE" />    </piece>    <piece name="curve6" class="piece">      <image file="curve45.png" hflip="false" vflip="true" />      <connector x="6" y="1" orientation="SW" />      <connector x="46" y="25" orientation="N" />    </piece>    <piece name="curve7" class="piece">      <image file="curve45.png" hflip="true" vflip="true" />      <connector x="0" y="17" orientation="S" />      <connector x="45" y="6" orientation="NW" />    </piece>    <piece name="curve8" class="piece">      <image file="curve45-2.png" hflip="true" vflip="true" />      <connector x="17" y="0" orientation="E" />      <connector x="1" y="40" orientation="SE" />    </piece>    <piece name="straight1" class="piece">      <image file="shorthorizstraight.png" hflip="false" vflip="false" />      <connector x="0" y="0" orientation="S" />      <connector x="32" y="8" orientation="N" />    </piece>    <piece name="straight2" class="piece">      <image file="straighthorizlong.png" hflip="false" vflip="false" />      <connector x="0" y="0" orientation="S" />      <connector x="48" y="8" orientation="N" />    </piece>    <piece name="straight3" class="piece">      <image file="shortvertstraight.png" hflip="false" vflip="false" />      <connector x="0" y="0" orientation="E" />      <connector x="8" y="32" orientation="W" />    </piece>    <piece name="straight4" class="piece">      <image file="straightvertlong.png" hflip="false" vflip="false" />      <connector x="0" y="0" orientation="E" />      <connector x="8" y="48" orientation="W" />    </piece>    <piece name="diagonal1" class="piece">      <image file="diagonalshort.png" hflip="false" vflip="false" />      <connector x="28" y="5" orientation="NW" />      <connector x="0" y="23" orientation="SE" />    </piece>    <piece name="diagonal2" class="piece">      <image file="diagonallong.png" hflip="false" vflip="false" />      <connector x="39" y="5" orientation="NW" />      <connector x="0" y="34" orientation="SE" />    </piece>    <piece name="diagonal3" class="piece">      <image file="diagonalshort.png" hflip="true" vflip="false" />      <connector x="5" y="0" orientation="SW" />      <connector x="23" y="28" orientation="NE" />    </piece>    <piece name="diagonal4" class="piece">      <image file="diagonallong.png" hflip="true" vflip="false" />      <connector x="5" y="0" orientation="SW" />      <connector x="34" y="39" orientation="NE" />    </piece>    <piece name="if1" class="piece">      <image file="if.png" hflip="false" vflip="false" />      <connector x="0" y="17" orientation="S" />      <connector x="45" y="6" orientation="NW" />      <connector x="37" y="25" orientation="N" /><!-- need to shorten graphic by 5 pixels -->    </piece>    <piece name="if2" class="piece">      <image file="if.png" hflip="true" vflip="false" />      <connector x="8" y="1" orientation="SW" />      <connector x="48" y="25" orientation="N" />      <connector x="11" y="17" orientation="S" /><!-- need to shorten graphic by 11 pixels -->    </piece>    <piece name="main" class="piece">      <image file="tunnel.png" hflip="false" vflip="false" />      <connector x="16" y="13" orientation="N" />    </piece>  </set>  <palette>    <tab name="track">      <piece ref="curve1" />      <piece ref="curve2" />      <piece ref="curve3" />      <piece ref="curve4" />      <piece ref="curve5" />      <piece ref="curve6" />      <piece ref="curve7" />      <piece ref="curve8" />      <piece ref="straight1" />      <piece ref="straight2" />      <piece ref="straight3" />      <piece ref="straight4" />      <piece ref="diagonal1" />      <piece ref="diagonal2" />      <piece ref="diagonal3" />      <piece ref="diagonal4" />    </tab>        <tab name="switches">      <piece ref="if1" />      <piece ref="if2" />    </tab>  </palette>    <land>    <piece ref="main" x="50" y="50" fixed="true" />  </land></mindtrains>